<?xml version="1.0" encoding="utf-8"?>
<ItemRenderer name="ServantOfKTeshArms" DungeonOnly="true">
	<Data>
		<Sprite name="sprite" fileName="game/particles4.rttex" textureSize="32" frame="2,2"/>
		<Animations>
			<SpriteAnimation name="slash_anim" sprite="sprite" animTime="200">
				<Frame>2,2</Frame>
				<Frame>3,2</Frame>
			</SpriteAnimation>
		</Animations>
		<ParticleSystems>
			<ParticleSystem name="projectile_particle" updateStage="OnCreate">
				<Emitter>
					<Set name="infParticles">true</Set>
					<Set name="infLifeTime">true</Set>
					<Set name="particlesPerEmission">1</Set>
					<Set name="checkParticleNumToEmit">true</Set>
					<Set name="particlesToEmit">1</Set>
				</Emitter>
				<Particle>
					<Set name="sprite">sprite</Set>
					<Set name="renderAlignment">LEFT_CENTER</Set>
					<Set name="isBeamParticle">true</Set>
					<Set name="blendingMode">PREMULTIPLIED_ALPHA</Set>
					<Set name="collisionDamageAmount">10</Set>
					<Set name="destroyOnCollision">false</Set>
					<Set name="collisionSize">32,32</Set>
					<Set name="initialScale">1,1</Set>
					<Set name="useTargetDir">false</Set>
					<Random name="lifeTime" min="0.6" max="0.8"/>
					<Set name="collisionType">
						<CollisionType>PROJECTILES_ENEMY</CollisionType>
					</Set>
					<Set name="collisionFlag">
						<CollisionFlag>PLAYERS</CollisionFlag>
					</Set>
				</Particle>
			</ParticleSystem>
		</ParticleSystems>
	</Data>
</ItemRenderer>