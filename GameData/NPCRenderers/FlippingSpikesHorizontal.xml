<?xml version="1.0" encoding="utf-8"?>
<ItemRenderer NPCType="FlippingSpikesHorizontal">
	<Data>
		<Sprite name="s_sprite" fileName="game/tiles_page17.rttex" textureSize="32" frame="8,11" />
		<Sprite name="s_sprite_temp" fileName="game/tiles_page17.rttex" textureSize="32" frame="8,11" />
		<Animations>
			<SpriteAnimation name="a_to_up_active" sprite="s_sprite" playOnState="NPC.Up" isLoop="false" animTime="400">
				<Frame>9,11</Frame>
				<Frame>8,11</Frame>
				<Frame>10,11</Frame>
				<Frame>11,11</Frame>
			</SpriteAnimation>
			<SpriteAnimation name="a_to_down_active" sprite="s_sprite" playOnState="NPC.Down" isLoop="false" animTime="400">
				<Frame>11,11</Frame>
				<Frame>10,11</Frame>
				<Frame>8,11</Frame>
				<Frame>9,11</Frame>
			</SpriteAnimation>
		</Animations>
		<StateMachines>
			<StateMachine name="NPC" ServerOnly="true">
				<States start="Up">
					<NPCStationary name="Up" ExitTime="2"/>
					<NPCStationary name="Down" ExitTime="2"/>
				</States>
				<Transitions>
					<Transition to="Up" from="Down"/>
					<Transition to="Down" from="Up"/>
				</Transitions>
			</StateMachine>
		</StateMachines>
		<Components>
			<Info/>
			<Enemy>
				<Set name="SoulsReward">0</Set>
				<Set name="destroyTileId">0</Set>
			</Enemy>
			<Movement>
				<Set name="IsStatic">true</Set>
			</Movement>
			<StateMachine/>
			<NPCRenderActionCmp/>
			<CollisionBox>
				<Set name="size">32,31</Set>
				<Set name="CollisionFlag">"PLAYERS|PROJECTILES|PROJECTILES_ENEMY"</Set>
				<Set name="CollisionType">"WORLD_TILES"</Set>
			</CollisionBox>
			<DebugDraw/>
		</Components>
		<ParticleSystems>
			<ParticleSystem name="fireParticle1_up">
				<Emitter>
					<Set name="offset">0,0</Set>
					<Set name="infParticles">true</Set>
					<Set name="infLifeTime">true</Set>
					<Set name="particlesPerEmission">1</Set>
					<Set name="particlesToEmit">1</Set>
					<Set name="checkParticleNumToEmit">true</Set>
					<Set name="delayTimer">0.1</Set>
				</Emitter>
				<Particle>
					<Set name="sprite">s_sprite_temp</Set>
					<Set name="blendingMode">PREMULTIPLIED_ALPHA</Set>
					<Set name="relativeToEmitter">false</Set>
					<Set name="collisionDamageAmount">40</Set>
					<Set name="destroyOnCollision">false</Set>
					<Set name="collisionSize">31.5,17</Set>
					<Set name="renderAlignment">DOWN_CENTER</Set>
					<Set name="lifeTime">1.9</Set>
					<Set name="initialAlpha">0</Set>
					<Set name="initialScale">1,1</Set>
					<Set name="collisionType">
						<CollisionType>PROJECTILES_ENEMY</CollisionType>
					</Set>
					<Set name="collisionFlag">
						<CollisionFlag>PLAYERS</CollisionFlag>
					</Set>
				</Particle>
			</ParticleSystem>			
			<ParticleSystem name="fireParticle1_down">
				<Emitter>
					<Set name="offset">0,0</Set>
					<Set name="infParticles">true</Set>
					<Set name="infLifeTime">true</Set>
					<Set name="particlesPerEmission">1</Set>
					<Set name="particlesToEmit">1</Set>
					<Set name="checkParticleNumToEmit">true</Set>
					<Set name="delayTimer">0.1</Set>
				</Emitter>
				<Particle>
					<Set name="sprite">s_sprite_temp</Set>
					<Set name="renderAlignment">UPPER_CENTER</Set>
					<Set name="blendingMode">PREMULTIPLIED_ALPHA</Set>
					<Set name="relativeToEmitter">false</Set>
					<Set name="collisionDamageAmount">40</Set>
					<Set name="destroyOnCollision">false</Set>
					<Set name="initialAlpha">0</Set>
					<Set name="collisionSize">31.5,17</Set>
					<Set name="lifeTime">1.9</Set>
					<Set name="initialScale">1,1</Set>
					<Set name="collisionType">
						<CollisionType>PROJECTILES_ENEMY</CollisionType>
					</Set>
					<Set name="collisionFlag">
						<CollisionFlag>PLAYERS</CollisionFlag>
					</Set>
				</Particle>
			</ParticleSystem>
		</ParticleSystems>
	</Data>
	<RenderRules>
		<Render>
			<SpriteRender name="s_sprite"/>
		</Render>
		<Update>
			<ParticleSystemUpdate name="fireParticle1_up" onState="NPC.Up" />
			<ParticleSystemUpdate name="fireParticle1_down" onState="NPC.Down" />
		</Update>
		<SwitchOffClothingParticles>
			<ParticleSystemOff name="fireParticle1_up"/>
			<ParticleSystemOff name="fireParticle1_down"/>
		</SwitchOffClothingParticles>
	</RenderRules>
</ItemRenderer>